{
	"schemaVersion": 1,
	"id": "bin-slot",
	"version": "${version}",
	"name": "Bin Slot",
	"description": "Delete items directly from your inventory",
	"authors": [
		"5antos"
	],
	"contact": {
		"homepage": "https://github.com/5antos/bin-slot/issues",
		"sources": "https://github.com/5antos/bin-slot"
	},
	"license": "MIT",
	"icon": "assets/binslot/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.p5antos.binslot.BinSlotModInitializer"
		],
		"client": [
			"me.p5antos.binslot.BinSlotClientModInitializer"
		]
	},
	"mixins": [
		"binslot.mixins.json",
		{
			"config": "binslot.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.7",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
